  obrir_terminal: obrir_terminal {
    compatible = "zmk,behavior-macro";
    label = "obrir_terminal";
    #binding-cells = <0>;
    bindings = <&kp LGUI(SPACE)>, <&macro_wait_time 100>, 
               <&kp T>, <&kp E>, <&kp R>, <&kp M>, <&kp I>, <&kp N>, <&kp A>, <&kp L>, <&kp ENTER>;
  };
    open_https_www: open_https_www {
    compatible = "zmk,behavior-macro";
    label = "open_https_www";
    #binding-cells = <0>;
    bindings = <&kp O>, <&kp P>, <&kp E>, <&kp N>
              , <&kp H>
              , <&kp T>
              , <&kp T>
              , <&kp P>
              , <&kp S>
              , <&kp LSHIFT(DOT)>
              , <&kp LSHIFT(7)>
              , <&kp LSHIFT(7)>
              , <&kp W>
              , <&kp W>
              , <&kp W>
              , <&kp .>;
  };
    obrir_olivereta: obrir_olivereta {
    compatible = "zmk,behavior-macro";
    label = "obrir_olivereta";
    #binding-cells = <0>;
    bindings = <&obrir_terminal>,  <&open_https_www>, 
    <&kp M>, <&kp E>, <&kp T>, <&kp E>, <&kp O>, <&kp L>, <&kp I>, <&kp V>, <&kp E>, <&kp R>,<&kp E>,<&kp T>,<&kp A>,<&kp DOT><&kp C>,<&kp O>,<&kp M>,<&kp ENTER>;
  };